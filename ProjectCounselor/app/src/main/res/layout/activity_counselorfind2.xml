
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/ll_menuLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:gravity="left"
            android:layout_marginTop="0dp"
            android:orientation="vertical"
            android:textColor="#ff000000" >

            <!-- include -->

            <include
                android:id="@+id/ic_leftslidemenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="54dp"
                layout="@layout/activity_slide" />
        </LinearLayout>

        <!-- slide layout -->

        <LinearLayout
            android:id="@+id/ll_mainlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="0dp"
            android:background="#ffffff"
            android:gravity="left"
            android:orientation="vertical" >





            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >


                <!-- top -->

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="41.8dp"
                    android:background="#2f438e">

                </LinearLayout>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="10.8dp"
                    android:src="@drawable/menu1"
                    android:text="menu"
                    android:id="@+id/menu" />


                <ImageView
                    android:layout_width="88dp"
                    android:layout_height="19dp"
                    android:layout_marginLeft="54dp"
                    android:layout_marginTop="10.8dp"
                    android:src="@drawable/findfind"

                    />

                <!-- top end -->


                <FrameLayout
                    android:layout_width="318dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    >

                    <ImageView
                        android:layout_width="96dp"
                        android:layout_height="51dp"

                        android:layout_marginTop="72dp"
                        android:src="@drawable/sel1"

                        />




                    <ImageView
                        android:layout_width="215.1dp"
                        android:layout_height="51dp"
                        android:layout_gravity="right"
                        android:layout_marginTop="72dp"
                        android:src="@drawable/sel2"

                        />



                </FrameLayout>


                <!-- 1 -->

            <FrameLayout
                android:layout_width="324.3dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                >

                <ImageView
                    android:layout_width="324.3dp"
                    android:layout_height="162dp"
                    android:layout_marginTop="141dp"
                    android:src="@drawable/se3"

                    />

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="161dp"
                    android:src="@drawable/start3"

                    />

                <ImageView
                    android:layout_width="55dp"
                    android:layout_height="19dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="176dp"
                    android:src="@drawable/conselor_name"

                    />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="78dp"
                    android:layout_marginTop="178dp"
                    android:src="@drawable/moon1"
                    android:id="@+id/moon1"

                    />

                <ImageView
                    android:layout_width="57dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="210dp"
                    android:src="@drawable/counso"

                    />

                <ImageView
                    android:layout_width="123dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="232dp"
                    android:src="@drawable/counso2"

                    />
                <ImageView
                    android:layout_width="173dp"
                    android:layout_height="12dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginTop="258dp"
                    android:src="@drawable/counso3"

                    />

                <ImageView
                    android:layout_width="59dp"
                    android:layout_height="59dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="35dp"
                    android:layout_marginTop="178dp"
                    android:src="@drawable/counso4"

                    />

                <ImageView
                    android:layout_width="54dp"
                    android:layout_height="18dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="37dp"
                    android:layout_marginTop="246dp"
                    android:src="@drawable/counso5"
                    android:id="@+id/counselor_reserve1"

                    />

                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="12dp"
                    android:layout_marginTop="208dp"
                    android:src="@drawable/counso6"
                    android:id="@+id/counselor1"

                    />


                 </FrameLayout>

                <!-- 2 -->
            <FrameLayout
                android:layout_width="324.3dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                >
                <ImageView
                    android:layout_width="324.3dp"
                    android:layout_height="162dp"
                    android:layout_marginTop="318dp"
                    android:src="@drawable/se3"

                    />

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="343dp"
                    android:src="@drawable/star1"

                    />

                <ImageView
                    android:layout_width="55dp"
                    android:layout_height="19dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="358dp"
                    android:src="@drawable/conselor_name"

                    />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="78dp"
                    android:layout_marginTop="359dp"
                    android:src="@drawable/moon2"
                    android:id="@+id/moon2"

                    />

                <ImageView
                    android:layout_width="57dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="392dp"
                    android:src="@drawable/counso"

                    />

                <TextView
                    android:layout_width="123dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="414dp"

                    android:textSize="16dp"
                    android:id="@+id/tv2"

                    />
                <ImageView
                    android:layout_width="173dp"
                    android:layout_height="12dp"
                    android:layout_marginLeft="18dp"
                    android:layout_marginTop="442dp"
                    android:src="@drawable/counso3"

                    />

                <ImageView
                    android:layout_width="59dp"
                    android:layout_height="59dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="35dp"
                    android:layout_marginTop="357dp"
                    android:src="@drawable/counso4"

                    />

                <ImageView
                    android:layout_width="54dp"
                    android:layout_height="18dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="37dp"
                    android:layout_marginTop="428dp"
                    android:src="@drawable/counso5"

                    />

                <ImageView
                    android:layout_width="12dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="12dp"
                    android:layout_marginTop="385dp"
                    android:src="@drawable/counso6"

                    />
                 </FrameLayout>



                <LinearLayout
                    android:id="@+id/ll_fragment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_empty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@android:color/transparent"
                    />


            </FrameLayout>


        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="56.5dp"
            android:src="@drawable/bottomline"
            />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|left"
            android:orientation="horizontal"
            android:layout_marginBottom="8.8dp"

            >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                >

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="34.3dp"
                    android:src="@drawable/home_1"
                    android:id="@+id/counselorfind"
                    />

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="19.5dp"
                    android:src="@drawable/home_2"
                    />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                >

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="19.5dp"
                    android:src="@drawable/home_3"
                    android:id="@+id/go0"
                    />

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="24.5dp"
                    android:src="@drawable/home_4"
                    android:id="@+id/find2_home"
                    />

            </FrameLayout>
        </LinearLayout>






    </FrameLayout>

</LinearLayout>