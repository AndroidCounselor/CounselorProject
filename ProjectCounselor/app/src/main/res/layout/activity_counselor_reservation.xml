
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/ll_menuLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:gravity="left"
            android:layout_marginTop="0dp"
            android:orientation="vertical"
            android:textColor="#ff000000" >

            <!-- include -->

            <include
                android:id="@+id/ic_leftslidemenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="54dp"
                layout="@layout/activity_slide" />
        </LinearLayout>

        <!-- slide layout -->

        <LinearLayout
            android:id="@+id/ll_mainlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="0dp"
            android:background="#ffffff"
            android:gravity="left"
            android:orientation="vertical" >





            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <!-- top -->

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="41.8dp"
                    android:background="#2f438e">

                </LinearLayout>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="10.8dp"
                    android:src="@drawable/menu1"
                    android:text="menu"
                    android:id="@+id/menu" />


                <ImageView
                    android:layout_width="88dp"
                    android:layout_height="19dp"
                    android:layout_marginLeft="54dp"
                    android:layout_marginTop="10.8dp"
                    android:src="@drawable/reserve_title"

                    />

                <!-- top end -->


                <!-- 1 -->


                <ImageView
                    android:layout_width="318dp"
                    android:layout_height="162dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="82dp"
                    android:src="@drawable/se3"

                    />

                <FrameLayout
                    android:layout_width="318dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal">


                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="10dp"
                        android:layout_marginLeft="19dp"
                        android:layout_marginTop="102dp"
                        android:src="@drawable/start3"

                        />

                    <ImageView
                        android:layout_width="55dp"
                        android:layout_height="19dp"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="117dp"
                        android:src="@drawable/conselor_name"

                        />

                    <ImageView
                        android:layout_width="16dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="78dp"
                        android:layout_marginTop="117dp"
                        android:src="@drawable/moon1"
                        android:id="@+id/moon1"

                        />

                    <ImageView
                        android:layout_width="57dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="17dp"
                        android:layout_marginTop="151dp"
                        android:src="@drawable/counso"

                        />

                    <TextView
                        android:layout_width="123dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="17dp"
                        android:layout_marginTop="173dp"
                        android:textSize="16dp"
                        android:id="@+id/tvtv"


                        />
                    <ImageView
                        android:layout_width="173dp"
                        android:layout_height="12dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="199dp"
                        android:src="@drawable/counso3"

                        />

                    <ImageView
                        android:layout_width="59dp"
                        android:layout_height="59dp"
                        android:layout_gravity="right"
                        android:layout_marginRight="14dp"
                        android:layout_marginTop="138dp"
                        android:src="@drawable/counso4"

                        />


                </FrameLayout>






                <!-- 2 -->

                <ImageView
                    android:layout_width="318dp"
                    android:layout_height="223dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="254dp"
                    android:src="@drawable/reserve_rec2"

                    />

                <!-- confrim button -->

                <ImageView
                    android:layout_width="163dp"
                    android:layout_height="51dp"
                    android:layout_marginBottom="80dp"
                    android:layout_gravity="center_horizontal|bottom"
                    android:src="@drawable/reserve_confirm"
                    android:id="@+id/reserve_confirm"

                    />



                <LinearLayout
                    android:id="@+id/ll_fragment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_empty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@android:color/transparent"
                    />


            </FrameLayout>


        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="56.5dp"
            android:src="@drawable/bottomline"
            />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|left"
            android:orientation="horizontal"
            android:layout_marginBottom="8.8dp"

            >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                >

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="34.3dp"
                    android:src="@drawable/home_1"
                    android:id="@+id/counselorfind"
                    />

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="19.5dp"
                    android:src="@drawable/home_2"
                    />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                >

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="left"
                    android:layout_marginLeft="19.5dp"
                    android:src="@drawable/home_3"
                    android:id="@+id/go0"
                    />

                <ImageView
                    android:layout_width="46dp"
                    android:layout_height="41dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="24.5dp"
                    android:src="@drawable/home_4"
                    android:id="@+id/reservehome"
                    />

            </FrameLayout>
        </LinearLayout>






    </FrameLayout>

</LinearLayout>